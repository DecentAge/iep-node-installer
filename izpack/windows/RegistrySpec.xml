<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<izpack:registry version="5.0"
                 xmlns:izpack="http://izpack.org/schema/registry"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://izpack.org/schema/registry http://izpack.org/schema/5.0/izpack-registry-5.0.xsd">
    <pack name="UninstallStuff">
        <value name="DisplayName"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="Infinity Economics"/>
        <value name="UninstallString"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="&quot;$INSTALL_PATH\jre\bin\javaw.exe&quot; -jar &quot;$INSTALL_PATH\uninstaller\uninstaller.jar&quot;"/>
        <value name="DisplayIcon"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="$INSTALL_PATH\html\www\wallet\assets\images\favicon.ico"/>
        <value name="HelpLink"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="https://infinity-economics.io"/>
        <value name="URLInfoAbout"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="$APP_URL"/>
       <value name="Publisher"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="DecentAge"/>
       <value name="Version"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="$APP_VER"/>
       <value name="DisplayVersion"
               keypath="SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\$APP_NAME"
               root="HKLM"
               string="$APP_VER"/>
    </pack>
</izpack:registry>
